<?xml version="1.0" encoding="UTF-8"?>
<pluginLoaderCollection>
    <pluginLoaderData>
        <id>OverlayPlugin</id>
        <class>io.opensphere.overlay.OverlayPlugin</class>
        <appVersion>1.0</appVersion>
        <requiredPluginDependency>IconLegendPlugin</requiredPluginDependency>
        <pluginProperty>
            <key>mousePosition</key>
            <value>true</value>
        </pluginProperty>
        <pluginProperty>
            <key>viewerPosition</key>
            <value>true</value>
        </pluginProperty>
        <pluginProperty>
            <key>colorBackground</key>
            <value>true</value>
        </pluginProperty>
        <pluginProperty>
            <key>timeDisplay</key>
            <value>true</value>
        </pluginProperty>
        <pluginProperty>
            <key>Denver</key>
            <value>false</value>
        </pluginProperty>
        <pluginProperty>
            <key>randomDots</key>
            <!-- The first part of this is the number of dots per set, the second part is the number of sets -->
            <value>10000,4</value>
        </pluginProperty>
        <pluginProperty>
            <key>latitudeLongitudeLines</key>
            <value>true</value>
        </pluginProperty>
        <pluginProperty>
            <key>mgrsgrid</key>
            <value>false</value>
        </pluginProperty>
    </pluginLoaderData>
    <pluginLoaderData>
